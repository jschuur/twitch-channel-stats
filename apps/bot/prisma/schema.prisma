generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model ChatMessage {
  id        Int      @id @default(autoincrement())
  type      String
  channel   String
  username  String?
  context   Json?
  text      String?
  createdAt DateTime @default(now())
}
